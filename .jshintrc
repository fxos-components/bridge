{
  "camelcase": false,
  "forin": false,
  "newcap": false,
  "noarg": true,
  "nonew": true,
  "quotmark": "single",
  "undef": true,
  "unused": "vars",
  "strict": true,
  "trailing": true,
  "maxlen": 80,

  "eqnull": true,
  "esnext": true,
  "expr": true,
  "globalstrict": true,

  "maxerr": 1000,
  "regexdash": true,
  "laxcomma": true,
